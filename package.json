{
  "name": "desmos-ast-viewer",
  "version": "2.1.2",
  "description": "View the AST of a Desmos mathematical expression.",
  "scripts": {
    "start": "parcel src/index.html --dist-dir build --open --port 3000",
    "build": "parcel build src/*.html --dist-dir build --public-url /desmos-ast-viewer --no-cache",
    "deploy": "gh-pages -d build"
  },
  "browserslist": {
    "production": ">0.2% and not dead and supports es6-module",
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "gh-pages": "^4.0.0",
    "parcel": "^2.7.0",
    "typescript": "^4.7.4"
  },
  "license": "MIT",
  "homepage": "desmos-ast-viewer/",
  "devDependencies": {
    "@types/desmos": "^1.6.0"
  }
}
